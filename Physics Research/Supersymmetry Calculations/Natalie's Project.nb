Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ChiralPartial", "[", 
    RowBox[{"r_", ",", "e_"}], "]"}], "=", 
   RowBox[{"QPochhammer", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "^", "r"}], " ", 
      RowBox[{"q", "^", 
       RowBox[{"(", 
        RowBox[{"r", "/", "2"}], ")"}]}], " ", 
      RowBox[{"x", "^", "e"}]}], ",", " ", "q"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ChiralPartialBar", "[", 
    RowBox[{"r_", ",", "e_"}], "]"}], "=", 
   RowBox[{"QPochhammer", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "^", 
       RowBox[{"(", "r", ")"}]}], " ", 
      RowBox[{"q", "^", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"r", "/", "2"}]}], ")"}]}], " ", 
      RowBox[{"x", "^", 
       RowBox[{"(", "e", ")"}]}]}], ",", " ", "q"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"FermiPartial", "[", 
    RowBox[{"r_", ",", "e_"}], "]"}], "=", 
   RowBox[{"QPochhammer", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"-", "1"}], ")"}], "^", "r"}]}], " ", 
      RowBox[{"q", "^", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"1", "/", "2"}], "+", 
         RowBox[{"r", "/", "2"}]}], ")"}]}], " ", 
      RowBox[{"x", "^", "e"}]}], ",", " ", "q"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"FermiPartialBar", "[", 
     RowBox[{"r_", ",", "e_"}], "]"}], "=", 
    RowBox[{"QPochhammer", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "^", 
         RowBox[{"(", "r", ")"}]}]}], " ", 
       RowBox[{"q", "^", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"1", "/", "2"}], "+", 
          RowBox[{"r", "/", "2"}]}], ")"}]}], " ", 
       RowBox[{"x", "^", 
        RowBox[{"(", "e", ")"}]}]}], ",", "q"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Chiral2dIndex", "[", 
    RowBox[{"r_", ",", "e_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"ChiralPartial", "[", 
       RowBox[{"r", ",", "e"}], "]"}], " ", 
      RowBox[{"ChiralPartialBar", "[", 
       RowBox[{
        RowBox[{"-", "r"}], ",", 
        RowBox[{"-", "e"}]}], "]"}]}], ")"}], "^", 
    RowBox[{"(", 
     RowBox[{"-", "1"}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Fermi2dIndex", "[", 
    RowBox[{"r_", ",", "e_"}], "]"}], "=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"FermiPartial", "[", 
      RowBox[{"r", ",", "e"}], "]"}], " ", 
     RowBox[{"FermiPartialBar", "[", 
      RowBox[{
       RowBox[{"-", "r"}], ",", 
       RowBox[{"-", "e"}]}], "]"}]}], ")"}]}], ";"}]}], "Input",
