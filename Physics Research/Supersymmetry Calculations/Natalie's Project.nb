Cell["Let us now define these operators in Mathematica:", "Text",
 CellChangeTimes->{{3.846261301141777*^9, 
  3.846261309446637*^9}},ExpressionUUID->"54ca412c-e087-4616-83b4-\
871fa0b6788c"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"XYZNeumannIndex", "[", 
    RowBox[{"r_", ",", "x_", ",", "e_"}], "]"}], "=", 
   RowBox[{"1", "/", 
    RowBox[{"QPochhammer", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "^", "r"}], " ", 
      RowBox[{"q", "^", 
       RowBox[{"(", 
        RowBox[{"r", "/", "2"}], ")"}]}], " ", 
      RowBox[{"a", "^", "x"}], " ", 
      RowBox[{"y", "^", "e"}]}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"XYZDirichletIndex", "[", 
    RowBox[{"r_", ",", "x_", ",", "e_"}], "]"}], "=", 
   RowBox[{"QPochhammer", "[", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "^", "r"}]}], " ", 
     RowBox[{"q", "^", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"1", "/", "2"}], "+", 
        RowBox[{"r", "/", "2"}]}], ")"}]}], 
     RowBox[{"a", "^", "x"}], " ", 
     RowBox[{"y", "^", "e"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.8462613195245485`*^9, 3.8462614374027095`*^9}, {
  3.8462615423616114`*^9, 3.8462615600541596`*^9}},
 CellLabel->"In[84]:=",ExpressionUUID->"8d2bd366-dcc8-46ad-86f4-63d1b8d87519"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"XYZNeumannIndex", "[", 
   RowBox[{"1", ",", 
    RowBox[{"-", "1"}], ",", "1"}], "]"}], " ", 
  RowBox[{"XYZNeumannIndex", "[", 
   RowBox[{"1", ",", 
    RowBox[{"-", "1"}], ",", 
    RowBox[{"-", "1"}]}], "]"}], " ", 
  RowBox[{"XYZDirichletIndex", "[", 
   RowBox[{"0", ",", "2", ",", "0"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.846261446398287*^9, 3.8462614722965603`*^9}, {
  3.846261573402689*^9, 3.8462616207557077`*^9}},
