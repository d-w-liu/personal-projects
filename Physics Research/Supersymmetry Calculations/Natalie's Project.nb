          StyleBox["z", "TI"], 
          StyleBox["n", "TI"]], 
         SubscriptBox[
          OverscriptBox["\[Gamma]", "_"], "-"]}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\partial^n_z \\bar{\\gamma}_-", "state" -> 
     "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "d3648988-d664-4f6c-ab9a-595e5314a4eb"],
 ", where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       SubscriptBox["\[Gamma]", "-"], TraditionalForm], "errors" -> {}, 
     "input" -> "\\gamma_-", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5c7957da-dbf2-41f1-84b9-ba591b82c6f0"],
 "is the leading fermion in the expansion of ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association[
    "boxes" -> FormBox["\[CapitalGamma]", TraditionalForm], "errors" -> {}, 
     "input" -> "\\Gamma", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "1ff4dc4b-6d21-4429-809d-9e5654409db7"],
 ". Let\[CloseCurlyQuote]s suppose here, in complement to the chiral \
multiplet above, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association[
    "boxes" -> FormBox["\[CapitalGamma]", TraditionalForm], "errors" -> {}, 
     "input" -> "\\Gamma", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "18c11968-f1a7-4cfe-9b81-7520b384c663"],
 " possesses flavor charge -1 and R-charge +1. The operators ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox["\[PartialD]", 
          StyleBox["z", "TI"], 
          StyleBox["n", "TI"]], 
         SubscriptBox["\[Gamma]", "-"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "\\partial^n_z \\gamma_-", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8adc0c15-e067-4e03-b8f3-734b3a5b455a"],
 " must have the same -1 flavor charge and 1 R-charge, and will possess spin ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["n", "TI"], "+", 
         FractionBox["1", "2"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "n+\\frac{1}{2}", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "a0f6be95-a17a-4890-96b9-3dde72182c3d"],
 " for each n. Their conjugates, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox["\[PartialD]", 
          StyleBox["z", "TI"], 
          StyleBox["n", "TI"]], 
         SubscriptBox[
          OverscriptBox["\[Gamma]", "_"], "-"]}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\partial^n_z \\bar{\\gamma}_-", "state" -> 
     "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "53ded7e0-7ee1-402e-afd6-3b44987035bc"],
 ", will flip the charges and keep the spin, so we\[CloseCurlyQuote]ll pick \
up +1 flavor charge and -1 R-charge, with the same spin. In this case, \
however, we are counting a ",
 StyleBox["fermionic",
  FontSlant->"Italic"],
 " variable, though, and so we use the \[OpenCurlyDoubleQuote]geometric \
series\[CloseCurlyDoubleQuote] ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{"1", "-", 
         StyleBox["x", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "1-x", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3c9402d3-420f-4c73-8360-ce31afd23f78"],
 " (here noting that fermionic variables truncate at ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         StyleBox["O", "TI"], "(", "1", ")"}], TraditionalForm], 
     "errors" -> {}, "input" -> "O(1)", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "1f4fdb27-f59f-4305-aad2-5804af847d76"],
 " since Grassmann numbers square to zero, and the factor of -1 in front of x \
comes about because the trace is taken with ",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       SuperscriptBox[
        RowBox[{"(", "-1", ")"}], 
        StyleBox["F", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "(-1)^F", "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5b76028e-8761-447d-9c4b-dc931327834b"],
 " where F is 0 for a bosonic variable and 1 for a fermionic variable). Thus, \
the count will become:\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["F", "TI"], 
          RowBox[{
            StyleBox["T", "TI"], ",", 
            StyleBox["B", "TI"]}]], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["x", "TI"], ";", 
             StyleBox["q", "TI"]}], ")"}], "\[LongEqual]", 
         UnderscriptBox["\[Product]", 
          RowBox[{
            StyleBox["n", "TI"], "\[GreaterEqual]", "0"}], LimitsPositioning -> 
          True], 
         RowBox[{"(", 
           RowBox[{"1", "-", 
             SuperscriptBox[
              StyleBox["q", "TI"], 
              StyleBox["n", "TI"]], 
             StyleBox["x", "TI"]}], ")"}], 
         RowBox[{"(", 
           RowBox[{"1", "-", 
             SuperscriptBox[
              StyleBox["q", "TI"], 
              RowBox[{
                StyleBox["n", "TI"], "+", "1"}]], 
             SuperscriptBox[
              StyleBox["x", "TI"], "-1"]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "F_{T,B} (x;q) = \\prod_{n\\geq0}(1-q^n x)(1-q^{n+1}x^{-1})", "state" -> 
     "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e35f402d-b963-4dde-bd35-47105357c8eb"],
 " (Eq. 3)\n\nWe can also shorten these expressions using q-Pochhammer \
symbols, which are defined as:\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          RowBox[{"(", 
            RowBox[{
              StyleBox["x", "TI"], ";", 
              StyleBox["q", "TI"]}], ")"}], "\[Infinity]"], "\[LongEqual]", 
         UnderscriptBox["\[Product]", 
          RowBox[{
            StyleBox["n", "TI"], "\[GreaterEqual]", "0"}], LimitsPositioning -> 
          True], 
         RowBox[{"(", 
           RowBox[{"1", "-", 
             SuperscriptBox[
              StyleBox["q", "TI"], 
              StyleBox["n", "TI"]], 
             StyleBox["x", "TI"]}], ")"}]}], TraditionalForm], "errors" -> {},
      "input" -> "(x;q)_{\\infty}=\\prod_{n\\geq0}(1-q^n x)", "state" -> 
     "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "43f97f13-8a02-4cda-bbf3-5d37061e2eeb"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[Association["boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          RowBox[{"(", 
            StyleBox["q", "TI"], ")"}], "\[Infinity]"], "\[LongEqual]", 
         UnderscriptBox["\[Product]", 
          RowBox[{
            StyleBox["n", "TI"], "\[GreaterEqual]", "1"}], LimitsPositioning -> 
          True], 
         RowBox[{"(", 
           RowBox[{"1", "-", 
             SuperscriptBox[
              StyleBox["q", "TI"], 
              StyleBox["n", "TI"]]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> "(q)_{\\infty}=\\prod_{n\\geq1}(1-q^n)", 
     "state" -> "Boxes"],
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "6cc21c5e-3f29-427d-9714-f055513f3d33"]
}], "Text",
