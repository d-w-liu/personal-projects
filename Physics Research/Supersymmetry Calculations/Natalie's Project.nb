 FractionBox[
  RowBox[{"QPochhammer", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["a", "2"]}], " ", 
     SqrtBox["q"]}], ",", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["a", "2"]}], " ", 
     SqrtBox["q"]}]}], "]"}], 
  RowBox[{
   RowBox[{"QPochhammer", "[", 
